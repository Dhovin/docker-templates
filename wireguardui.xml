<?xml version="1.0"?>
<Container version="2">
  <Name>Wireguard UI</Name>
  <Repository>embarkstudios/wireguard-ui</Repository>
  <Registry>https://registry.hub.docker.com/r/embarkstudios/wireguard-ui</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>true</Privileged>
  <Support></Support>
  <Project>https://pritunl.com/</Project>
  <Overview>Wireguard UI</Overview>
  <Category>Network:VPN Status:Beta</Category>
  <WebUI>https://[IP]:[PORT:8080]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/Dhovin/docker-templates/main/WireguardUI.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/Dhovin/docker-pritunl/master/logo.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Description>Wireguard UI</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>5555</HostPort>
        <ContainerPort>5555</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/Pritunl</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="HTTPS" Target="8080" Default="8080" Mode="tcp" Description="Container Port: 8080" Type="Port" Display="always" Required="true" Mask="false">8080</Config>
  <Config Name="Wireguard" Target="5555" Default="5555" Mode="udp" Description="Container Port: 5555" Type="Port" Display="always" Required="true" Mask="false">5555</Config>
  <Config Name="Key 1" Target="PUID" Default="99" Mode="" Description="Container Variable: PUID" Type="Variable" Display="advanced-hide" Required="false" Mask="false">99</Config>
  <Config Name="Key 2" Target="PGID" Default="100" Mode="" Description="Container Variable: PGID" Type="Variable" Display="advanced-hide" Required="false" Mask="false">100</Config>
</Container>