<?xml version="1.0"?>
<Container version="2">
  <Name>Wireguard UI</Name>
  <Repository>embarkstudios/wireguard-ui</Repository>
  <Registry>https://registry.hub.docker.com/r/embarkstudios/wireguard-ui</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>true</Privileged>
  <Support></Support>
  <Project>https://pritunl.com/</Project>
  <Overview>Wireguard UI</Overview>
  <Category>Network:VPN Status:Beta</Category>
  <WebUI>https://[IP]:[PORT:8080]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/Dhovin/docker-templates/main/WireguardUI.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/Dhovin/docker-pritunl/master/logo.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Description>Wireguard UI</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>5555</HostPort>
        <ContainerPort>5555</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/Pritunl</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
</Container>