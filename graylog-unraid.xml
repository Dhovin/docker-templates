<?xml version="1.0"?>
<Container version="2">
  <Name>Graylog</Name>
  <Repository>dhovin/graylog-unraid</Repository>
  <Registry>https://hub.docker.com/r/graylog/graylog</Registry>
  <Network>bridge</Network>
  <MyIP />
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support></Support>
  <Project></Project>
  <Overview>Graylog 4.0 server</Overview>
  <Category>Network:Other Status:Beta</Category>
  <WebUI>http://[IP]:[PORT:9000]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/Dhovin/docker-templates/main/graylog-unraid.xml</TemplateURL>
  <Icon></Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DonateText/>
  <DonateLink/>
  <Description>Graylog 4.0 server</Description>
  <Environment>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
      <Mode/>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
      <Mode/>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9000</HostPort>
        <ContainerPort>9000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>12201</HostPort>
        <ContainerPort>12201</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>1514</HostPort>
        <ContainerPort>1514</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/Pritunl</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
    <Labels/>
 </Container>